module Sensor {
	struct Reading {
		long time;
		long date;
		long waterLevel;
		long alarmLevel;
	};
	
	typedef sequence<Reading> ReadingLog;
	
	interface Sensor {
		boolean isFlooding();
		boolean isActive();

		long getWaterLevel();
		long getAlarmLevel();

		void setAlarmLevel(in long alarmLevel);
		void setWaterLevel(in long waterLevel);
		void setActive(in boolean active);
		void checkAlarmStatus();
		
		ReadingLog getLog();
		
	};
	
	
};